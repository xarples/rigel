type Client {
  id: ID! @unique
  firstName: String!
  lastName: String
  email: String!
  username: String! @unique
  password: String!
  users: [User]
}

type User {
  id: ID! @unique
  firstName: String!
  lastName: String
  email: String
  username: String! @unique
  password: String!
  client: Client!
}
